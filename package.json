  "dependencies": {
    "@cmsgov/design-system": "2.0.0-beta.1",
    "concurrently": "^5.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
